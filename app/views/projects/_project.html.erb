<div id="<%= dom_id project %>">
  <p>
    <strong>Title:</strong>
    <%= project.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= project.image %>
  </p>

  <p>
    <strong>Source url:</strong>
    <%= project.source_url %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= project.created_at %>
  </p>

  <p>
    <strong>Update at:</strong>
    <%= project.update_at %>
  </p>

  <p>
    <strong>Rating:</strong>
    <%= project.rating %>
  </p>

</div>
